#include<stdio.h>
#include<string.h>
#include<cs50.h>

int main(void)
{
    string s="abcdefghijklmnopqrstuvwxyz"; //input key
    char q[26];
    int a = strlen(s);
    if (a!=26) //make sure key is correct length
    {
        printf("Invalid key");
        return 1;
    }
    for(int i=0; i<a; i++) //turn key into lower case alphabet
    {
        if (s[i] >= 'a' && s[i] <='z')
        {
            q[s[i]-96]=s[i];
        }
        else if (s[i] >= 'A' && s[i] <='Z')
        {
            q[s[i]-64]=s[i]+32;
        }
    }
    char z[26]="abcdefghijklmnopqrstuvwxyz"; //compare alphabetical key to alphabet
    printf("%s\n",q);
    printf("%s\n",z);
    int j=0;
    for (int i=0; i<26; i++)
    {
        if(q[i]==z[i])
        {
        j++;
        }
    }
    printf("%i",j);
    return 0;
}
