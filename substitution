#include<stdio.h>
#include<string.h>
#include<cs50.h>

int main(void)
{
    int ret; //initialise string compare
    string s="abcdefghijklmnopqrstuvwxyz"; //input key
    char q[26];
    int a = strlen(s);
    if (a!=26) //make sure key is correct length
    {
        printf("Invalid key");
        return 1;
    }
    for(int i=0; i<a; i++) //turn key into lower case alphabet
    {
        if (s[i] >= 'a' && s[i] <='z')
        {
            q[s[i]-96]=s[i];
        }
        else if (s[i] >= 'A' && s[i] <='Z')
        {
            q[s[i]-64]=s[i]+32;
        }
    }
    string w="abcdefghijklmnopqrstuvwxyz";
    string r=q; //go from char array to string
    printf("%s\n",w);
    printf("%s\n",r);
    ret = strcmp(r,w);
    if (ret==0) {printf("The strings are equal");}
    if (ret!=0) {printf("The strings are NOT equal");}
    return 0;
}
